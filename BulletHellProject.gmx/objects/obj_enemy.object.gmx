        <arguments>
          <argument>
            <kind>1</kind>
            <string>if type == 1 {
 
 //Process for creating Bullets
 
    //Creates an array for the bullet instances and the angles each bullet uses
    var i;
    i = 0;
    repeat(8)
    {
        bul[i] = 0;
        ang[i] = 0;
        i += 1;
    }
    
    //Fills the bullet angle array with all the angles it will start with
    var angle = 0;
    i = 0;
    repeat(8)
    {
        ang[i] = angle;
        i += 1;
        angle += 45;
    }
    
    //Fills bullet instance array
    i = 0;
    repeat(8)
    {
        bul[i] = instance_create(x,y,obj_bullet);
        bul[i].direction = ang[i] + bullet_change;
        bul[i].speed = 2;
        bul[i].image_angle = bul[i].direction;
        i += 1;
    }
    bullet_change += 4;
    
} else if type == 2 {
    
    bullet = instance_create(x,y,obj_bullet);
    
    if bullet_change &lt;= -15
    {
        left_or_right = true;
    }
    else if bullet_change &gt;= 15
    {
        left_or_right = false;
    }
    
    if left_or_right == true
    {
        bullet_change += 1;
    }
    else if left_or_right == false
    {
        bullet_change -= 1;
    }
    
    bullet.direction = bullet_change - 90;
    bullet.image_angle = bullet.direction;
    bullet.speed = 5;
    
} else if type == 3 {

    bullet = instance_create(x,y,obj_bullet);
    
    bullet.direction = -90;
    bullet.image_angle = bullet.direction;
    bullet.speed = 7.5;
    
}

alarm[0] = firing_speed
</string>
